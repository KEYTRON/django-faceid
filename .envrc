# Использовать локальный Python через pyenv/uv/poetry — как вы привыкли.
# Минимально: system-python + venv
layout python

export DJANGO_SECRET_KEY="dev-only-change-me"
export DJANGO_DEBUG="1"

# InsightFace/ONNXRuntime могут подкачать модели — оставляем кеш в проекте
export INSIGHTFACE_HOME="$PWD/.insightface"
